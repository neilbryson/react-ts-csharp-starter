  "version": "1.0.0",
  "dependencies": {
    "axios": "^0.21.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.4",
    "redux": "^4.1.1",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@neilbryson/eslint-config": "^4.0.3",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^27.0.1",
    "@types/node": "^14.17.11",
    "@types/react": "^17.0.19",
    "@types/react-dom": "^17.0.9",
    "@types/react-redux": "^7.1.18",
    "autoprefixer": "^10.3.2",
    "eslint": "^7.32.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "husky": "^7.0.1",
    "jest": "^27.0.6",
    "lint-staged": "^11.1.2",
    "postcss": "^8.3.6",
    "prettier": "^2.3.2",
    "react-is": "^17.0.2",
    "redux-devtools-extension": "^2.13.9",
    "tailwindcss": "^2.2.7",
    "ts-jest": "^27.0.5",
    "typescript": "^4.3.5",
    "vite": "^2.5.0",
    "vite-plugin-checker": "^0.3.4"
  },
  "scripts": {
    "build": "vite build",
    "prepare": "cd .. && husky install client/.husky",
    "preview": "vite preview",
    "start": "vite",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.ts?(x)",
      "!src/index.ts?(x)",
      "!**/node_modules/**"
    ],
    "rootDir": "src",
    "preset": "ts-jest",
    "setupFilesAfterEnv": [
      "./setupTests.ts"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$/": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "testEnvironment": "jsdom"
  }
}
